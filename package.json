{
  "name": "cbc",
  "productName": "cbc",
  "version": "0.9.0",
  "description": "CBC Electron application",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "npm run lint && ts-node scripts/dev-server.ts",
    "publish": "npm run lint && npm run build:r && npm run publish:m",
    "lint": "eslint --ext .ts .",
    "build:r": "vue-tsc -p tsconfig.renderer.json --noEmit && vite build --emptyOutDir",
    "publish:m": "tsc -p tsconfig.main.json && electron-forge publish",
    "make": "tsc -p tsconfig.main.json && electron-forge make",
    "dev": "vite",
    "preview": "vite preview"
  },
  "keywords": [],
  "repository": "git@github.com:joehecn/cbc.git",
  "author": {
    "name": "joehecn",
    "email": "leanbrown@live.cn"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cbc"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "joehecn",
              "name": "cbc"
            },
            "prerelease": false,
            "draft": false
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "ali-oss": "^6.16.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "electron": "^16.0.5",
    "element-plus": "^1.3.0-beta.1",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "mqtt": "^4.3.4",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.30.1",
    "vuex": "^4.0.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
